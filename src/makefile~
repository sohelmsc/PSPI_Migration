# Author: Sohel Bhuiyan 
# Makefile to link the library and create the executable file

CC=g++
CFLAGS=-O2 -Wall -ansi -pedantic

LFLAGS= $(PRELFLAGS) -L$L -lm -lfftw3

all: Migration

OBJ= Migration.o SeismicImaging.o

Migration: ${OBJ}
	$(CC) -o Migration ${OBJ} $(CFLAGS) $(INCL) $(LDFLAGS) $(LFLAGS)

Migration.o: Migration.cpp 
	${CC} -c Migration.cpp $(FLAG) $(OPTC) 

SeismicImaging.o: SeismicImaging.cpp
	${CC} -c SeismicImaging.cpp $(FLAG) $(OPTC)
clean:
	rm life hpp 2>&-

